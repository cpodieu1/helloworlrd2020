pipeline {
    agent any
     stages {
      stage('build'){
        steps {
          echo "hello word"
            sleep 4
        }
      }
     stages {
      stage('test'){
        steps {
          echo "deploy word"
            sleep 4
        }
      }
     stages {
      stage('deploy'){
        steps {
          echo "test"
            sleep 4
        }
      }
     stages {
      stage('time'){
        steps {
          echo "word"
            sleep 4
        }
      }
    }
}
